//PRACTICE 67
#include <iostream>
#include <algorithm>
using namespace std;
int main(){
	int a,b,c,d,n,mn,mx;
	cin>>n;
	a = n/1000;
	b = n%1000/100;
	c = n%100/10;
	d = n%10;
	mx = a;
	mn = a;
	int mni = 0, mxi=0;
	int arr[4] = {a,b,c,d};
	for(int i = 0; i<4; i++){
		if(mn>arr[i]){
			mn = arr[i];
			mni = i;
		}
		if(mx<arr[i]){
			mx = arr[i];
			mxi = i;
		}
	}
	swap(arr[mni], arr[mxi]);
	for(int i = 0; i<4; i++){
		cout<<arr[i];
	}
}
//PRACTICE 68
#include <iostream>
#include <algorithm>
#include <cmath>
using namespace std;
int main(){
	double a,b,c,d,x2,x1;
	cin>>a>>b>>c;
	d=pow(b,2)-4*a*c;
	x1=(-b+sqrt(d))/(2*a);
	x2=(-b-sqrt(d))/(2*a);
	cout<<x1<<" "<<x2;
	return 0;
}
//PRACTICE 69
#include <iostream>
#include <algorithm>
#include <cmath>
using namespace std;
int main(){
	double a,b,c,s,p;
	cin>>a>>b>>c;
	if (a+b>c && a+c>b && b+c>a) {
		p=(a+b+c)/2;
		s=sqrt(p*(p-a)*(p-b)*(p-c));
		cout<<s;
	}
	else {
		cout<<"Not triangle";
	}
	return 0;
}
//PRACTICE 70
#include <iostream>
#include <algorithm>
#include <cmath>
