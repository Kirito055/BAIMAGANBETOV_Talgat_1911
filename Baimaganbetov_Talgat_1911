#include <iostream>

using namespace std;
int main() {
    int k;
    cin>> k;
for(int n=2;n<=999;n++){
    if((1+n)*n==2*k){
      int a=n; 
        cout<<a;
         break;
    }
    
}

return 0;
}
//PRACTICE 134
#include <iostream>
#include <cstdio>
using namespace std;
int main()
{
long long x,y, s=0;
       double r;
cin>>r; 
for(x=0;-r<x,x<r;x++)
{
for(y=0;-r<y,y<r;y++)
{
if(x*x+y*y<=r*r)
{
s++;
}
}
}
cout<<s + 1;
return 0;
}
//PRACTICE 135
