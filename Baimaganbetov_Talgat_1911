using namespace std;
int main () {
    int a, b, c, d;
    int sum = 0;
    for(int i = 1000; i <= 9999; i++) {
        a =  i / 1000;
        b = (i % 1000) / 100;
        c = (i % 100) / 10;
        d = i % 10;
        sum = a + b + c + d;
        if(sum == 15)
            cout << i << " ";
    }
    return 0;
}
//PRACTICE 120
#include <iostream>
using namespace std;
int main () {
    int n, mx;
    cin >> n;
    mx = n % 10;
    n = n / 10;
    while (n > 0) {
        if (n % 10 > mx)
            mx = n % 10;
        n = n / 10;
    }
    cout << mx << endl;
    return 0;
}
