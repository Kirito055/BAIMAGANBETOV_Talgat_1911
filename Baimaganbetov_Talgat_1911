//Practice 3
#include <iostream>
using namespace std;
int main()
{
    int b,count = 1;
    cin>>b;
    for (int i = 0; i < b; i++)
    {
        for(int j = 0; j < count; j++)
        cout << '0';
        count++;
        cout<<endl;
    }
    return 0;
}


//PRACTICE 4
#include <iostream>
using namespace std;
int main()
{
    int b,c,count = 1;
    cin>>b;
    cin>>c;
    for (int i = 0; i < b; i++)
    {
        for(int j = 0; j < c; j++)
        cout << 'A';
        count++;
        cout<<endl;
    }
    return 0;
}

//PRACTICE 5
#include <iostream>
using namespace std;
int main(){
  for(int i=0; i<4; i++){
  	if(i==0){
  		cout<<"*"<<"     "<<"*"<<"     "<<"*";
	  }
	if(i==1){
  		cout<<" *"<<"   "<<"*"<<" "<<"*"<<"   "<<"*";
	  }
	if(i==2){
  		cout<<"  *"<<" "<<"*"<<"   "<<"*"<<" "<<"*";
	  }
	if(i==3){
		cout<<"   *"<<"     "<<"*";
	}
	cout<<endl;
  }
  return 0;
 }

