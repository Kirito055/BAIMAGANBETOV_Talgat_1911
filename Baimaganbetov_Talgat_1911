int main()
{
  for(int i=100000;i<=999999;i++){
       int a,b, c,d,e,f,z,x, v,n,m,l;
  a=(i/100000)%10;
  b=(i/10000)%10;
  c=(i/1000)%10;
  d=(i/100)%10;
  e=(i/10)%10;
  f=i%10;
    z=(i/100000)%10;
  x=((i+1)/10000)%10;
  v=((i+1)/1000)%10;
  n=((i+1)/100)%10;
  m=((i+1)/10)%10;
  l=(i+1)%10;
  if((a+b+c+d+e+f)%13==0&&(a1+b1+c1+d1+e1+f1)%13==0){
      cout<<i<<" "<<(i+1)<<" "<<"Yes";cout<<endl;
  }
  
  }
	return 0;
}
//PRACTICE 131
#include <iostream>
#include <cmath>
using namespace std;

int main()
{
for(int i=2;i<10000;i++){
     int b, c,d,e,f;
  b=(i/10000)%10;
  c=(i/1000)%10;
  d=(i/100)%10;
  e=(i/10)%10;
  f=i%10;
  int sum=b+c+d+e+f;
  for(int j=2; j<100;j++){
      if(i==pow(sum,j)){
      
          cout<<i<<endl; 
      }
   
         
      } }
 
	return 0;
}
