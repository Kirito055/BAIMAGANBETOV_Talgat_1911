//PRACTICE 49
#include <iostream>
using namespace std;
int main(){
	double a, b, c, d, max;
	cin>>a>>b>>c>>d;

	if (a)
	if(a==b && b==c && c==d){
		cout<<"not found";
	}
	else{ max = a>b&&(a%2==0)?		a>c?a>d&&(d%2==0)?a:d:c>d&&(c%2==0)?c:d:b>c?b>d?b:d:c>d?c:d;
	cout<<max; 
	}
	return 0;
}

