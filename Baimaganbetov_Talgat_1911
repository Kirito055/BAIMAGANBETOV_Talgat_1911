//PRACTICE 110
#include <iostream>
using namespace std;
int main () {
    string a= "AAA";
    string b= "BBB";
    int n, m;
    cin >> n >> m;
    for (int i = 0; i < n; i++){
        if (i % 2 == 0) {
            for(int j = 0; j < m; j++)
                cout << a << b;
        }
        else {
            for(int j=0; j<m; j++)
                cout << b << a;
        }
        cout << endl;
    }
    return 0;
}
//PRACTICE 111
#include <iostream>
using namespace std;
int main () {
    char a = 'A', b = 'B';
    int n;
    cin >> n;
    for (int i = 0; i < n; i++) {
        for(int j = 0; j < 2*n; j++) {
            if(i == 0 || i == n-1)
                cout << a;
            else if(j == 0 || j == n*2 - 1)
                cout << a;
            else
                cout << b;
        }
        cout << endl;
    }
    return 0;
}
